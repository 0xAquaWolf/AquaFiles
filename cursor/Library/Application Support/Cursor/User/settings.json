{
  "code-runner.clearPreviousOutput": true,
  "code-runner.runInTerminal": true,
  "code-runner.saveFileBeforeRun": true,
  "python.condaPath": "/opt/homebrew/Caskroom/miniforge/base/envs/hyperliquid/bin/python",
  "python.terminal.activateEnvironment": true,
  "window.zoomLevel": 1,
  "workbench.colorTheme": "Catppuccin Macchiato",
  "editor.fontFamily": "OperatorMonoSSmLig Nerd Font",
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "name": "envKeys",
        "scope": "string.quoted.single.ini,constant.numeric.ini,string.quoted.double.ini",
        "settings": {
          "foreground": "#19354900"
        }
      }
    ]
  },
  "editor.minimap.enabled": false,
  "notebook.formatOnSave.enabled": true,
  "notebook.codeActionsOnSave": {
    "notebook.source.fixAll": "explicit",
    "notebook.source.organizeImports": "explicit"
  },
  "editor.formatOnSave": true,
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": [
        ">"
      ],
      "after": [
        ">",
        "g",
        "v"
      ]
    },
    {
      "before": [
        "<"
      ],
      "after": [
        "<",
        "g",
        "v"
      ]
    },
  ],
  "explorer.confirmDragAndDrop": false,
  "terminal.integrated.inheritEnv": false,
  "extensions.experimental.affinity": {
    "vscodevim.vim": 1
  },
  "editor.fontSize": 14,
  "debug.console.fontSize": 14,
  "chat.editor.fontSize": 14,
  "terminal.external.osxExec": "WezTerm.app",
  "terminal.integrated.fontSize": 14,
  "workbench.iconTheme": "catppuccin-macchiato",
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnSave": true
  },
  "files.associations": {
    "*.php": "php",
    "*.csv": "plaintext"
  },
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb",
    "*.sqlite": "${capture}.${extname}-*",
    "*.db": "${capture}.${extname}-*",
    "*.sqlite3": "${capture}.${extname}-*",
    "*.db3": "${capture}.${extname}-*",
    "*.sdb": "${capture}.${extname}-*",
    "*.s3db": "${capture}.${extname}-*"
  },
  "dataWrangler.enabledFileTypes": {
    "csv": false
  },
  // "[typescriptreact]": {
  //   "editor.defaultFormatter": "esbenp.prettier-vscode"
  // },
  // "[typescript]": {
  //   "editor.defaultFormatter": "esbenp.prettier-vscode"
  // },
  // "[javascript]": {
  //   "editor.defaultFormatter": "esbenp.prettier-vscode"
  // },
}